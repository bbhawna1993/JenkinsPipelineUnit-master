pipeline {
         agent any
         stages {
                 stage('One') {
                 steps {
                     echo 'Hi, this is Zulaikha from edureka'
                 }
                 }
                 stage('Two') {
                 steps {
                    input('Do you want to proceed?')
                 }
                 }
                 stage('Three') {
                 steps {
                       echo "hello third stage"
                 }
                 }
                  
                 stage('four') {
                 steps {
                       echo "hello fourth stage"
                 }
                 }
                  stage('five') {
                 steps {
                       echo "hello five stage"
                 }
                 }
                 
              }
}
